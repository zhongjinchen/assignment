    <style>
        header {
            border-bottom: 1px solid #e7e7e7;
            position: fixed;
            top: 0;
            right: 0;
            left: 0;
            height: 60px;
            background-color: #fff;
            z-index: 999;
        }

            header > span {
                float: left;
                margin-top: 6px;
                margin-left: 50px;
            }

            header > div {
                float: left;
                margin-top: 20px;
