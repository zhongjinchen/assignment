            }

        .dropdown {
            float: right;
        }

            .dropdown > li {
                position: relative;
                margin-right: 50px;
                display: inline-block;
            }

        a {
            text-decoration: none;
        }

        .dropdown > li > a {
            color: #777;
        }

        .dropdown-content a {
            color: black;
        }

        .dropdown-content {
            display: none;
            position: absolute;
            list-style: none;
            border: 1px solid rgba(0,0,0,.15);
            width: 100px;
            background-color: #fff;
            float: left;
            right: 0px;
        }

        .dropdown > li:hover .dropdown-content {
            display: block;
        }

        .help-feedback a {
            border: 0px;
            background-color: rgba(128,255,212,.7);
            color: #23527c;
            position: fixed;
            bottom: 100px;
            right: 40px;
            padding: 10px;
            border-radius: 0px 10px;
        }

        .bottom {
            bottom: 100px;
        }
    </style>
